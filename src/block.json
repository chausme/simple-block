{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "create-block/simple-block",
    "version": "0.1.0",
    "title": "Simple Block",
    "category": "text",
    "icon": "flag",
    "description": "A simple Gutenberg block that doesn't do anything specific at the moment",
    "attributes": {
        "message": {
            "type": "string",
            "source": "text",
            "selector": "div"
        }
    },
    "supports": {
        "html": false
    },
    "textdomain": "simple-block",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}
